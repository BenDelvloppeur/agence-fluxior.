---
import Container from "../ui/Container.astro";
import MagicCTA from "../ui/MagicCTA.astro";
---

<section id="contact" class="py-20 relative border-t border-white/5 bg-background">
  <Container>
    
    <!-- Magic Premium CTA -->
    <div>
        <MagicCTA 
            title="Prêt à passer au <span class='text-gradient'>niveau supérieur ?</span>"
